    "cross-fetch": "^3.1.5",
    "eslint-config-next": "13.1.1",
    "next": "13.1.1",
    "next-sanity": "^4.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
